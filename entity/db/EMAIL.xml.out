<?xml version = "1.0"?>
<!DOCTYPE addressbook SYSTEM "addressbook.dtd" >
<table name="EMAIL" package="jwm.entity" >   <column formparameter="emailId" name="EMAILID" type="generated_int" variable="emailId" />
